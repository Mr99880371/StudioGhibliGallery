{
    "compilerOptions": {
      "target": "es5",
      "lib": ["dom", "esnext"],
      "module": "commonjs",
      "moduleResolution": "node",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "noEmit": true,
      "typeRoots": ["node_modules/@types"],
      "types": ["node", "jest"],
      "jsx": "react-jsx" 
    },
    "include": ["src/**/*"],
    "exclude": ["node_modules"]
  }
  